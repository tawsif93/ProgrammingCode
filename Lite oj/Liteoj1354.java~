	{
		String firstLine, binaryLine;
		int t;
		
		Scanner liteoj = new Scanner(System.in) ;
		
		t = liteoj.nextInt();
		
		for (int i = 0; i < t; i++ ) 
		{
			firstLine = liteoj.nextLine();
			binaryLine = liteoj.nextLine();
			
		}
			
		
	}
